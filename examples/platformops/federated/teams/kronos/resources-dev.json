{
  "metadata": {
    "format_version": "1.0.0",
    "type": "konnect.team.resources",
    "region": "eu",
    "team": "Kronos",
    "description": "Kronos team Konnect resources in the EU region"
  },
  "resources": [
    {
      "type": "konnect.control_plane",
      "name": "kronos-dev",
      "description": "Kronos development control plane",
      "labels": {
        "env": "dev"
      }
    },
    {
      "type": "konnect.control_plane",
      "name": "kronos-dev-azure-eu",
      "cloud_gateway": true,
      "description": "Kronos development control plane on Azure in the EU region",
      "labels": {
        "env": "dev"
      }
    },
    {
      "type": "konnect.cloud_gateway_network",
      "name": "kronos-azure-eu-network",
      "cidr_block": "10.0.0.0/16",
      "region": "germanywestcentral",
      "cloud_vendor": "azure",
      "availability_zones": ["germanywestcentral-az1", "germanywestcentral-az2", "germanywestcentral-az3"],
      "public_labels": {
        "team": "kronos"
      }
    },
    {
      "type": "konnect.cloud_gateway_configuration",
      "control_plane_geo": "eu",
      "api_access": "private+public",
      "control_plane_name": "kronos-dev-azure-eu",
      "dataplane_groups": [
        {
          "provider": "azure",
          "region": "germanywestcentral",
          "autoscale": {
            "configuration_data_plane_group_autoscale_autopilot": {
              "kind": "autopilot",
              "base_rps": 10,
              "max_rps": 100
            }
          },
          "cloud_gateway_network_name": "kronos-azure-eu-network"
        }
      ],
      "version": "3.10"
    }
  ]
}
